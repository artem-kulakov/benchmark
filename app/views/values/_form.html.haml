= form_for @value do |f|
  - if @value.errors.any?
    #error_explanation
      %h2= "#{pluralize(@value.errors.count, "error")} prohibited this value from being saved:"
      %ul
        - @value.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :indicator
    = f.text_field :indicator
  .field
    = f.label :report
    = f.text_field :report
  .field
    = f.label :value
    = f.number_field :value
  .actions
    = f.submit 'Save'
